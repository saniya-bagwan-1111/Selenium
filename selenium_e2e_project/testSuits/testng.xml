<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests"><!--- parallely run test we can run methods also parallelly-->

<listeners>
<listener class-name="com.seleniumlearning.testComponents.Listeners"></listener>
</listeners>

	<test name="Submit Order Test">
		<classes>
		<class name="com.seleniumlearning.e2eTest.SubmitOrderTest"/>
		</classes>
	</test>
  <test thread-count="5" name="Error Validation Test">
    <classes>
      <class name="com.seleniumlearning.e2eTest.ErrorValidationTest"/>
    </classes>
  </test> 
  
</suite> <!-- Suite -->
